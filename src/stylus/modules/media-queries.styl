	body
		path("m-bg-bubles.png")
	.estoy
		&-blanco,
		&-rojo
			display inline-block
			float right
			width 85%
			a
				display block
				width 100%
	.redes-desk
		width 50%
		text-align center
		margin-left 25%
		li
			width 30%
@media (max-width:1024px) and (max-height: 730px)
	.estoy-blanco,
	.estoy-rojo
		display block
		width: 35%
		float none
@media (max-width:1023px)
	.estoy-blanco,
	.estoy-rojo
		display inline-block
		float right
@media (max-width:1119px)
	.redes-desk
