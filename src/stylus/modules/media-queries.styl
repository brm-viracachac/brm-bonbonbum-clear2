@media (min-width:200px)
	#facebook,
	#instagram,
	#youtube 
		fill white
		width 100%
	#btn-play
		margin-left 32%
		margin-top 15%
		position absolute
		width 30%
	.btn-submit
		transitionBase()
		&:hover
			color $Rojobbb !important
			border-color $Rojobbb !important
	.container
		&-items
			background-color lighten($Azul, 10%)
			height 100%
			left -28rem
			padding-top 3rem
			position fixed
			width 28rem
			z-index 9
			transitionBase()
			&.active
				display block
				left 0rem
			#ht
				padding 1rem 3rem
				fill $Rojobbb	
		&-footer-fixed
			position fixed
			bottom 0
			width 100%
	.content
		&-video
			margin-top 2rem
			margin-bottom 8rem
			&:before, &:after
				border-radius: 50%;
				border: 0.1rem solid white;
				bottom -6rem
				color white
				display block
				height 4rem
				padding-top: 1rem;
				position absolute
				text-align center
				width: 4rem;
			&:before
				content '<'
				left 36%
			&:after
				content '>'
				right 36%
			&-redes
				display none
	.invisible-button
		position absolute
		display block
		width 100%
		height 100%
		cursor pointer
	.form
		margin-bottom 6rem
		&-nombre,
		&-email,
		&-numero,
		&-mensaje
			margin 1rem 0
			input, textarea
				background-color lighten($Azulbbb, 40%)
				border 0
				padding 1rem 2rem 1rem 8rem
				width 100%
				color Black
				transitionBase()
				&:focus, &:hover, &:visited, &:active
					z-index 1
					background-color lighten($Azulbbb, 70%)
				
			label
				position absolute
				padding 1rem 2rem

		&-aceptar
			input,
			p
				display inline-block
				text-align center
			input
				background-color lighten($Azulbbb, 40%)
				border 0
				position absolute
				width 10%
			p
				width 80%
				margin-top 0
			.btn-submit
				background-color transparent
				border 0.1rem solid white
				border-radius 0
				bottom 1rem
				color white
				margin-bottom 2rem
				margin-top 1rem
				position relative
				right 0rem
				width 100%

	.img-responsive
		width 100%
	.img
		&-de-que-estas-hecho,
		&-unico-como-tu
			position fixed
		&-de-que-estas-hecho
			bottom -6rem
			width 77%
		&-unico-como-tu
			bottom 3rem
			right -2rem
			width 60%
		
	.glyphicon.glyphicon-arrow-left
		color white
		font-size 1.5rem
		position absolute
		right 2rem
		top 1rem
		cursor pointer
		transitionBase()
		&:hover
			color $Rojobbb
	.glyphicon.glyphicon-remove-circle
		color white
		font-size 3rem
		float right
		margin-bottom 1rem
		cursor pointer		
	.logo
		&-bbb,
		&-colombina,
		&-clear
			width 32.333%
			span
				background-size 326%
				width 8rem
				margin 0 auto
		&-clear
			span
				background-position -8.6rem
		&-colombina
			span
				background-position -18.1rem
	.modal-backdrop.fade.in
		z-index 20
	.modal-dialog
		background-color none
		border-radius none
		pointer-events none
		z-index 21
	.modal-content
		background-color transparent !important
		border 0
		box-shadow none !important
		.user-instagram
			color white
			font-size 2rem
			text-align right
	.nav
		&-left, &-right
			position relative
			li
				width 100%
				a
					border-bottom 0.1rem solid white
					display block
					font-weight bold
					padding-bottom 2rem
					text-align center
					vertical-align middle
					width 100%
		&-left
			margin-bottom 0
		&-right
			margin-top 0
	.overlay-items
		display none
		&.active
			display block
	.parrafo
		&-article
			text-align center
		&-terminos
			text-align justify
	.redes
		margin-top 33rem
		text-align center
		li
			width 15%
	.redes-desk
		display none
	.terminos-footer
		display none
	.titulo
		&-article,
		&-video-des,
		&-terminos,
		&-contacto
			font-size 2rem
			text-align center
		&-terminos
			padding-bottom 2rem
			border-bottom 0.1rem solid white
		
	h2.titulo-producto
		font-size 0
	.thumb-instagram
		position relative
		padding 0
		width 100%
		text-align right
		font-size 2rem
		color black
		img
			-moz-object-fit cover
			-ms-object-fit cover
			-o-object-fit cover
			-webkit-object-fit cover
			height 24rem
			object-fit cover
			width 100%
		.user-instagram
			color black
			font-weight bold
